{
    "compilerOptions": {
      "baseUrl": ".",
      "target": "ES2022",
      "module": "CommonJS",
      "rootDir": ".",
      "outDir": "./out/",
      "moduleResolution": "node",
      "sourceMap": true,
      "useDefineForClassFields": true,
      "allowJs": false,
      "importHelpers": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "noUncheckedIndexedAccess": true,
    }
  }
  